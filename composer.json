{
    "name": "maxposter/dac-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DacBundle",
    "homepage": "http://maxposter.ru",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Vorobiev",
            "email": "avorobiev@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Svel",
            "email": "astahov@maxposter.ru",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.9",
        "symfony/symfony": "~3.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/maxposter/dac_test.git"
        }
    ],
    "require-dev": {
        "maxposter/dac-test-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Maxposter\\DacBundle": "" }
    },
    "target-dir": "Maxposter/DacBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
